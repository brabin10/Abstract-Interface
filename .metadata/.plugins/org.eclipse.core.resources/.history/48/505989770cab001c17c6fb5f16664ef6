package com.revature;

//Importing the Peson Class, so that we can meke objects of type Person, and access it s members(variables/methods)
import com.revature.models.Person;

public class Launcher {

	public static void main(String[] args) {
		
		System.out.println("Using our Constructors");
		
		//Instantiate a new Person object, using no-args constructor
		//Reference variable
		Person tyler = new Person();
		
		System.out.println(tyler.name);
		System.out.println(tyler.age);
		System.out.println(tyler.weight);
		
		//let's use the talk() method 
		System.out.println(tyler.talk("Hi I'm Tyler"));
	}

}
